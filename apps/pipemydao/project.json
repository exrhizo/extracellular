{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/pipemydao",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/pipemydao",
        "outputPath": "dist/apps/pipemydao"
      },
      "configurations": {
        "development": {
          "outputPath": "tmp/apps/pipemydao"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "pipemydao:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "pipemydao:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "pipemydao:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "pipemydao:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/pipemydao"],
      "options": {
        "jestConfig": "apps/pipemydao/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/pipemydao/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
