{
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "peacock.color": "#2f3620",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#4b5633",
    "activityBar.background": "#4b5633",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#182328",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#4b5633",
    "statusBar.background": "#2f3620",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#4b5633",
    "statusBarItem.remoteBackground": "#2f3620",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#2f3620",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#2f362099",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
